            android:orientation="horizontal">

            <!-- 기본관리 -->
            <Button
                android:id="@+id/button_menu01"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/home_menu_btn01"
                android:onClick="onClickDefaultManage" />
            <!--                android:drawableTop="@drawable/ic_baseline_auto_awesome_motion_24"
            -->
            <!-- 매출속보 -->
            <Button
